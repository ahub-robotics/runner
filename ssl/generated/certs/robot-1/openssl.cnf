[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = v3_req

[dn]
C = US
ST = State
L = City
O = Robot Runner
OU = Robots
CN = robot-1

[v3_req]
keyUsage = keyEncipherment, dataEncipherment, digitalSignature
extendedKeyUsage = serverAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = robot-1
DNS.3 = robot-1.local
IP.1 = 127.0.0.1
IP.2 = 0.0.0.0
IP.3 = 192.168.1.100
